    },
    "bugs": {
        "url": "https://github.com/acevedodamian/Asimov01/issues"
    },
    "engines": {
        "node": ">= 0.10.0"
    },
    "inceptionYear": 2014,
    "licenses": [
        {
            "type": "Apache",
